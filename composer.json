{
    "name": "cakedc/favorites",
    "description": "Favorites Plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "favorites", "cakedc"],
    "homepage": "https://github.com/CakeDC/favorites",
    "license": "MIT",
    "authors": [
        {
            "name": "CakeDC",
            "homepage": "http://www.cakedc.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/CakeDC/favorites/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/CakeDC/favorites/issues",
        "source": "https://github.com/CakeDC/favorites"
    },
    "require": {
        "cakephp/cakephp": "^3.5",
        "cakephp/migrations": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Favorites\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Favorites\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
